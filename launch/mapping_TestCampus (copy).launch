    <param name="vehicle_pos_head" value="0.0" />

    <param name="scan_range_min" value="0.50" />
    <param name="scan_range_max" value="60.0" />
    <param name="scan_dir_invert" value="true" />
    <param name="scan_angle_min" value="-1.658063" />
