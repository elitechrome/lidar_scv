<launch>
  <node pkg="lidar_scv" name="lidar_scv_campus_node" type="lidar_scv_node" >
    <param name="topic_LiDAR_rear" value="/lidar1/scan" />
    <param name="topic_LiDAR_front" value="/lidar2/scan" />
    <param name="topic_LiDAR_left" value="/lidar3/scan" />
