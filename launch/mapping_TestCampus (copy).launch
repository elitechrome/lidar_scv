    <param name="bag_period" value="0.0" />

    <param name="debug_mode_road" value="0" />

    <param name="vehicle_width" value="3.0" />
    <param name="vehicle_length" value="4.0" />

    <param name="vehicle_pos_x" value="0.0" />
